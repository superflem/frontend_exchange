{
  "name": "frontend_exchange",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.26.0",
    "concurrently": "^7.0.0",
    "html-react-parser": "^1.4.8",
    "js-sha3": "^0.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compilaApp": "tsc --outDir ./src/build --jsx react-jsx ./src/typescript/App.tsx",
    "compilaLogin": "tsc --outDir ./src/build --jsx react-jsx ./src/typescript/Login.tsx",
    "compilaNavbar": "tsc --outDir ./src/build --jsx react-jsx ./src/typescript/Navbar.tsx",
    "compilaNavbarLoggato": "tsc --outDir ./src/build --jsx react-jsx ./src/typescript/NavbarLoggato.tsx",
    "compilaSignup": "tsc --outDir ./src/build --jsx react-jsx ./src/typescript/Signup.tsx",
    "compilaLoggato": "tsc --outDir ./src/build --jsx react-jsx ./src/typescript/Loggato.tsx",
    "compilaHome": "tsc --outDir ./src/build --jsx react-jsx ./src/typescript/Home.tsx",
    "compilaSoldi": "tsc --outDir ./src/build --jsx react-jsx ./src/typescript/Soldi.tsx",
    "compilaDeposito": "tsc --outDir ./src/build --jsx react-jsx ./src/typescript/Deposito.tsx",
    "compilaWithdraw": "tsc --outDir ./src/build --jsx react-jsx ./src/typescript/Withdraw.tsx",
    "compilaBuy": "tsc --outDir ./src/build --jsx react-jsx ./src/typescript/Buy.tsx",
    "compilaList": "tsc --outDir ./src/build --jsx react-jsx ./src/typescript/ListTransactions.tsx",
    "compila": "concurrently \"npm run compilaApp\" \"npm run compilaLogin\" \"npm run compilaNavbar\" \"npm run compilaNavbarLoggato\" \"npm run compilaSignup\" \"npm run compilaLoggato\" \"npm run compilaHome\" \"npm run compilaSoldi\" \"npm run compilaDeposito\" \"npm run compilaWithdraw\" \"npm run compilaBuy\" \"npm run compilaList\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
